{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "types/**/*",
    "src/**/*",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "emitDeclarationOnly": true,
    "declaration": true,
    "declarationDir": "types",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
